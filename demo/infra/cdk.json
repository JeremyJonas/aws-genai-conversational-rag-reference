{
  "app": "npx ts-node -P tsconfig.json --prefer-ts-exts src/main.ts",
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "repositoryName": "galileo",
    "@aws-galileo/EULA/ENABLED": false,
    "ApplicationName": "Galileo",
    "ChatDomain": "Legal",
    "IncludeSampleDataset": true,
    "DefaultModelId": "falcon-lite",
    "FoundationModels": [
      "falcon-lite"
    ],
    "WebsiteContentPath": "../website/build",
    "CorpusDockerImagePath": "../corpus"
  },
  "output": "cdk.out",
  "build": "npx projen bundle",
  "watch": {
    "include": [
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules"
    ]
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}
